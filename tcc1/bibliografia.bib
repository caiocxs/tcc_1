% ===============================================================
% BIBLIOGRAFIA - TEMPLATE TCC1 SENAC SANTO AMARO
% ===============================================================
% Este arquivo contém exemplos de referências no padrão ABNT
% (NBR 6023:2018) para diferentes tipos de fontes.
%
% O pacote abntex2cite formata automaticamente as referências.
% Basta adicionar suas entradas aqui e usar \cite ou
% \citeonline no texto.
%
% TIPOS DE ENTRADA E FORMATO ABNT:
% -----------------------------------------------------------------
% book          -> SOBRENOME, Nome. Titulo. Edicao. Local: Editora, Ano.
% article       -> SOBRENOME, Nome. Titulo do artigo. Periodico, v. X, n. Y, p. XX-YY, Ano.
% inproceedings -> SOBRENOME, Nome. Titulo. In: EVENTO, Local. Anais. Editora, Ano. p. XX-YY.
% mastersthesis -> SOBRENOME, Nome. Titulo. Ano. Dissertacao (Mestrado) - Instituicao, Local, Ano.
% phdthesis     -> SOBRENOME, Nome. Titulo. Ano. Tese (Doutorado) - Instituicao, Local, Ano.
% misc          -> SOBRENOME, Nome. Titulo. Ano. Disponivel em: URL. Acesso em: data.
% manual        -> ORGANIZACAO. Titulo. Local, Ano.
% ===============================================================


% ---------------------------------------------------------------
% EXEMPLOS DE LIVROS
% Formato ABNT: SOBRENOME, Nome. Titulo: subtitulo. Edicao. Local: Editora, Ano.
% ---------------------------------------------------------------

% Livro com um autor
@book{Wazlawick2020,
  author    = {Raul Sidnei Wazlawick},
  title     = {Metodologia de Pesquisa para Ciência da Computação},
  publisher = {Elsevier},
  year      = {2020},
  edition   = {3},
  address   = {Rio de Janeiro}
}

% Livro com dois autores
@book{Pressman2016,
  author    = {Roger S. Pressman and Bruce R. Maxim},
  title     = {Engenharia de Software: uma abordagem profissional},
  publisher = {McGraw-Hill},
  year      = {2016},
  edition   = {8},
  address   = {Porto Alegre}
}

% Livro com um autor (outro exemplo)
@book{Sommerville2019,
  author    = {Ian Sommerville},
  title     = {Engenharia de Software},
  publisher = {Pearson},
  year      = {2019},
  edition   = {10},
  address   = {São Paulo}
}

% Livro traduzido
@book{Gamma2000,
  author    = {Erich Gamma and Richard Helm and Ralph Johnson and John Vlissides},
  title     = {Padrões de Projeto: soluções reutilizáveis de software orientado a objetos},
  publisher = {Bookman},
  year      = {2000},
  address   = {Porto Alegre},
  note      = {Tradução de Luiz A. Meirelles Salgado}
}

% Livro em inglês
@book{Cormen2009,
  author    = {Thomas H. Cormen and Charles E. Leiserson and Ronald L. Rivest and Clifford Stein},
  title     = {Introduction to Algorithms},
  publisher = {MIT Press},
  year      = {2009},
  edition   = {3},
  address   = {Cambridge}
}


